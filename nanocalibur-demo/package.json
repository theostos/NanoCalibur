{
  "name": "sample-tiled-webpack",
  "version": "1.0.0",
  "description": "Sample Excalibur Tiled Plugin with Webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "start:polling": "WATCHPACK_POLLING=true webpack-dev-server --open",
    "build": "webpack",
    "build:prod": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "serve": "http-server ./dist",
    "headless:compile": "rm -rf .nanocalibur_headless && npx -p typescript tsc src/nanocalibur_generated/*.ts src/nanocalibur_generated/canvas/*.ts --target ES2020 --module commonjs --moduleResolution node --outDir .nanocalibur_headless && cp src/nanocalibur_generated/game_spec.json .nanocalibur_headless/game_spec.json",
    "headless:server": "npm run headless:compile && node scripts/run_session_server.js",
    "session:auto": "node scripts/run_session_stack.js",
    "dummy:random": "node scripts/dummy_random_client.js",
    "human:cli": "node scripts/human_keyboard_client.js",
    "human:play": "node scripts/run_human_web.js"
  },
  "author": {
    "name": "Excalibur.js Team",
    "email": "excaliburjs@gmail.com",
    "url": "http://excaliburjs.com"
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "@excaliburjs/plugin-tiled": "0.30.1",
    "excalibur": "0.30.2"
  },
  "devDependencies": {
    "file-loader": "^6.2.0",
    "copy-webpack-plugin": "^13.0.1",
    "ts-loader": "9.5.1",
    "typescript": "5.7.2",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0",
    "http-server": "14.1.1"
  }
}
